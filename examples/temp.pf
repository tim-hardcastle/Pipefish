newtype

Z = clone{i int} int :
    0 <= that
    that < i

Foo = clone int using + :
    that < 8

def

(x Z{5}) + (y Z{5}) -> Z{5} :
    Z{5}(int(x) + int(y) mod 5)


newtype

Vec = clone{i int} list using + :
    len(that) == i

Qux = struct{i int} (zort int) :
    that[zort] < i

Moo = struct(zort int)

make Vec{3}, Z{5}, Qux{11}


